{
  "name": "@srph/react-timesheet",
  "version": "0.0.5",
  "description": "An opinionated timesheet component for React",
  "main": "index.js",
  "scripts": {
    "build": "webpack --verbose --progress",
    "build:production": "cross-env NODE_ENV=production webpack -p --verbose --colors --progress",
    "example:start": "webpack --config=examples/webpack.config.js --progress",
    "example:watch": "webpack --config=examples/webpack.config.js --progress -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build && npm run build:production"
  },
  "keywords": [
    "react",
    "timesheet"
  ],
  "author": "Kier Borromeo <seraphipod@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.4",
    "moment": "^2.15.1",
    "react": "^15.3.2",
    "react-click-outside": "github:tj/react-click-outside",
    "react-dom": "^15.3.2",
    "react-draggable": "^2.2.2",
    "react-link-state": "^0.1.2",
    "react-resizable": "^1.4.5",
    "react-select": "^1.0.0-rc.3"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "cross-env": "^3.1.4",
    "uuid": "^3.0.1",
    "webpack": "2.1.0-beta.25"
  }
}
